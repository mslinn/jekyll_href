{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "args": [
        "serve",
          "--drafts",
          "--force_polling",
          "--future",
          "--host", "0.0.0.0",
          "--incremental",
          "--livereload",
          "--livereload_port", "35744",
          "--port", "4444",
          "--strict_front_matter",
          "--unpublished"
      ],
      "cwd": "${workspaceRoot}/demo",
      "debugPort": "0",
      "env":{"JEKYLL_ENV": "development"},
      "name": "Debug Demo",
      "request": "launch",
      "script": "${workspaceRoot}/binstub/jekyll",
      "type": "rdbg",
      "useBundler": true,
    },
    {
      "debugPort": "45373",
      "name": "Attach rdbg",
      "request": "attach",
      "type": "rdbg",
    },
    {
      "args": [
        "-I",
        "${workspaceRoot}"
      ],
      "debugPort": "0",
      "name": "RSpec - all",
      "request": "launch",
      "script": "${workspaceRoot}/binstub/rspec",
      "type": "rdbg",
      "useBundler": true,
    },
    {
      "args": [
        "-I",
        "${workspaceRoot}",
        "${file}"
      ],
      "debugPort": "0",
      "name": "RSpec - active spec file only",
      "request": "launch",
      "script": "${workspaceRoot}/binstub/rspec",
      "type": "rdbg",
      "useBundler": true,
    }
  ]
}
